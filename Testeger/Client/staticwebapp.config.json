{
    "routes": [
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/api/configuration",
        "allowedRoles": [ "anonymous" ]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/_framework/*", "/css/*"]
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }